{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/(.*)": "<rootDir>/../src/$1",
    "@common": "<rootDir>/../src/common",
    "@modules": "<rootDir>/../src/modules",
    "@modules/(.*)": "<rootDir>../src/modules/$1",
    "@apis/(.*)": "<rootDir>/../src/apis/$1",
    "packageJson": "<rootDir>/../package.json",
    "src/(.*)": "<rootDir>../src/$1"
  }
}